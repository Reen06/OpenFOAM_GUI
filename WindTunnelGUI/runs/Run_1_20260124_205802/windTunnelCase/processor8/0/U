FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            noSlip;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 0();
    }
    model
    {
        type            noSlip;
    }
    procBoundary8to1
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to2
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to5
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to6
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to7
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to9
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to10
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary8to11
    {
        type            processor;
        value           uniform (0 0 0);
    }
